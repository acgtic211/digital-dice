{
  "thing_id": "smart_greenhouse_001",
  "timings": 60000,
  "default_values": {
    "property.temperature": 22.5,
    "property.humidity": 65,
    "property.light_level": 800,
    "property.soil_moisture": 75,
    "property.co2_level": 450,
    "property.fan_status": "off",
    "property.irrigation_status": "off",
    "property.light_status": "on",
    "property.window_status": "closed",
    "property.emergency": false
  },
  "behaviour": [
    {
      "description": "Temperature fluctuation based on time of day",
      "inputs": [],
      "outputs": ["property.temperature"],
      "distribution": {
        "type": "normal",
        "mean": 23,
        "standard_deviation": 1.5
      }
    },
    {
      "description": "Humidity is affected by temperature",
      "inputs": ["property.temperature"],
      "outputs": ["property.humidity"],
      "arithmeticExpression": {
        "eval_exp": "85 - property.temperature * 0.8"
      }
    },
    {
      "description": "CO2 level natural variation",
      "inputs": [],
      "outputs": ["property.co2_level"],
      "distribution": {
        "type": "discrete",
        "min": 400,
        "max": 600
      }
    },
    {
      "description": "Soil moisture decreases over time",
      "inputs": ["property.soil_moisture"],
      "outputs": ["property.soil_moisture"],
      "arithmeticExpression": {
        "eval_exp": "property.soil_moisture - 0.5"
      }
    },
    {
      "description": "Irrigation system activates when soil moisture is low",
      "inputs": ["property.soil_moisture"],
      "outputs": ["property.irrigation_status"],
      "conditional": {
        "eval_exp": "property.soil_moisture < 50",
        "value": "on"
      }
    },
    {
      "description": "Irrigation system deactivates when soil moisture is sufficient",
      "inputs": ["property.soil_moisture"],
      "outputs": ["property.irrigation_status"],
      "conditional": {
        "eval_exp": "property.soil_moisture >= 70",
        "value": "off"
      }
    },
    {
      "description": "Soil moisture increases when irrigation is on",
      "inputs": ["property.irrigation_status", "property.soil_moisture"],
      "outputs": ["property.soil_moisture"],
      "conditional": {
        "eval_exp": "property.irrigation_status == 'on'",
        "arithmeticExpression": {
          "eval_exp": "property.soil_moisture + 5"
        }
      }
    },
    {
      "description": "Light level cycles through day/night pattern",
      "inputs": [],
      "outputs": ["property.light_level"],
      "list": {
        "values": [0, 50, 200, 600, 900, 1200, 1400, 1300, 1000, 700, 400, 200, 0],
        "index": 0
      }
    },
    {
      "description": "Artificial lighting turns on when light level is low",
      "inputs": ["property.light_level"],
      "outputs": ["property.light_status"],
      "conditional": {
        "eval_exp": "property.light_level < 300",
        "value": "on"
      }
    },
    {
      "description": "Artificial lighting turns off when light level is high",
      "inputs": ["property.light_level"],
      "outputs": ["property.light_status"],
      "conditional": {
        "eval_exp": "property.light_level >= 300",
        "value": "off"
      }
    },
    {
      "description": "Fan activates when temperature is high",
      "inputs": ["property.temperature"],
      "outputs": ["property.fan_status"],
      "conditional": {
        "eval_exp": "property.temperature > 26",
        "value": "on"
      }
    },
    {
      "description": "Fan deactivates when temperature is normal",
      "inputs": ["property.temperature"],
      "outputs": ["property.fan_status"],
      "conditional": {
        "eval_exp": "property.temperature <= 24",
        "value": "off"
      }
    },
    {
      "description": "Temperature decreases when fan is on",
      "inputs": ["property.fan_status", "property.temperature"],
      "outputs": ["property.temperature"],
      "conditional": {
        "eval_exp": "property.fan_status == 'on'",
        "value": 30
      }
    },
    {
      "description": "Window opens when temperature is high and humidity is low",
      "inputs": ["property.temperature", "property.humidity"],
      "outputs": ["property.window_status"],
      "conditional": {
        "eval_exp": "property.temperature > 28",
        "value": "open"
      }
    },
    {
      "description": "Window closes when temperature is normal",
      "inputs": ["property.temperature"],
      "outputs": ["property.window_status"],
      "conditional": {
        "eval_exp": "property.temperature <= 28",
        "value": "closed"
      }
    },
    {
      "description": "Emergency condition when temperature is extremely high",
      "inputs": ["property.temperature"],
      "outputs": ["property.emergency"],
      "conditional": {
        "eval_exp": "property.temperature > 35",
        "value": true
      }
    },
    {
      "description": "Reset emergency condition when temperature is safe",
      "inputs": ["property.temperature"],
      "outputs": ["property.emergency"],
      "conditional": {
        "eval_exp": "property.temperature < 30",
        "value": false
      }
    }
  ]
}
